{
    "name": "ava-cn/laravel-vue-tailwind-purgecss",
    "description": "A simple way to install Vue, Tailwind and PurgeCSS",
    "keywords": [
        "ava-cn",
        "curder",
        "laravel-vue-tailwind-purgecss"
    ],
    "homepage": "https://github.com/ava-cn/laravel-vue-tailwind-purgecss",
    "license": "MIT",
    "authors": [
        {
            "name": "curder",
            "email": "q.curder@gmail.com",
            "homepage": "https://github.com/curder",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/contracts": "^5.6|^6.0|^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "AvaCN\\LaravelVTP\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "AvaCN\\LaravelVTP\\LaravelVTPServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
